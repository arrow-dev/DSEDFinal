@model IEnumerable<DSEDFinal.Models.Organization>
@{
    ViewBag.Title = "MyOrganizations";
}

<h2>My Organizations</h2>
<p>@Html.ActionLink("Add an Organization", "Create", new { }, new { @class = "btn btn-primary" })</p>
<ul>
    @if (Model.Count() == 0)
    {
        <li>You don't have any organizations yet.</li>
    }
    @foreach (var organization in Model)
    {
        <li>
            <a href=@Url.Action("Details", "Organization", new {id = organization.Id})>@organization.Name</a>
        </li>
    }
</ul>

