@model IEnumerable<DSEDFinal.Models.Organization>
@{
    ViewBag.Title = "MyMemberships";
}

<h2>My Memberships</h2>
<ul class="organization-list">
    @if (Model.Count() == 0)
    {
        <li>You don't have any Memberships yet. <br> Contact your Organizations Administrator to join.</li>
    }
    @foreach (var organization in Model)
    {
        <li>
            <a href=@Url.Action("Details", "Organization", new {id = @organization.Id})>@organization.Name</a>
        </li>
    }
</ul>

